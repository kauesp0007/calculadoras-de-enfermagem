    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const resultadoDiv = document.getElementById("resultado");
        const btnCalcular = document.getElementById("btnCalcular");
        const btnLimpar = document.getElementById("btnLimpar");
        const btnGerarPDF = document.getElementById("btnGerarPDF");
        const btnNandaNicNoc = document.getElementById("btnNandaNicNoc");
        const conteudoDiv = document.getElementById("conteudo");

        // Dados da escala de Bishop
        const bishopData = [
          {
            category: "Dilata√ß√£o do Colo do √ötero",
            nameAttr: "dilatacao",
            options: [
              { description: "0 cm", score: 0 },
              { description: "1-2 cm", score: 1 },
              { description: "3-4 cm", score: 2 },
              { description: "‚â• 5 cm", score: 3 },
            ],
            explain:
              "Abertura do colo do √∫tero, medida em cent√≠metros. Quanto maior a dilata√ß√£o, maior a pontua√ß√£o.",
          },
          {
            category: "Apagamento Cervical",
            nameAttr: "apagamento",
            options: [
              { description: "0-30%", score: 0 },
              { description: "40-50%", score: 1 },
              { description: "60-70%", score: 2 },
              { description: "‚â• 80%", score: 3 },
            ],
            explain:
              "Afinamento e encurtamento do colo do √∫tero em rela√ß√£o ao seu comprimento original. Expresso em porcentagem. Quanto maior a porcentagem, maior a pontua√ß√£o.",
          },
          {
            category: "Consist√™ncia Cervical",
            nameAttr: "consistencia",
            options: [
              { description: "Firme", score: 0 },
              { description: "M√©dia", score: 1 },
              { description: "Amolecida", score: 2 },
            ],
            explain:
              "Determina a firmeza do colo do √∫tero ao toque. Um colo mais amolecido est√° mais maduro e pronto para o parto.",
          },
          {
            category: "Posi√ß√£o do Colo do √ötero",
            nameAttr: "posicao",
            options: [
              { description: "Posterior", score: 0 },
              { description: "Mediana", score: 1 },
              { description: "Anterior", score: 2 },
            ],
            explain:
              "Localiza√ß√£o do colo do √∫tero em rela√ß√£o ao eixo p√©lvico. Um colo em posi√ß√£o anterior indica maior maturidade.",
          },
          {
            category: "Esta√ß√£o da Apresenta√ß√£o Fetal",
            nameAttr: "estacao",
            options: [
              { description: "-3", score: 0 },
              { description: "-2", score: 1 },
              { description: "-1 ou 0", score: 2 },
              { description: "+1 ou +2", score: 3 },
            ],
            explain:
              "Posi√ß√£o da cabe√ßa do beb√™ em rela√ß√£o √†s espinhas isqui√°ticas da pelve materna. A esta√ß√£o √© medida em cent√≠metros, sendo 0 o ponto de refer√™ncia. Quanto mais para baixo (+), maior a pontua√ß√£o.",
          },
        ];

        // Preenche o formul√°rio dinamicamente
        bishopData.forEach((item) => {
          const sectionDiv = document.createElement("div");
          sectionDiv.className = "bishop-criterion-section";
          sectionDiv.id = `section-${item.nameAttr}`;

          const titleH3 = document.createElement("h3");
          titleH3.className = "bishop-criterion-title";
          titleH3.innerHTML =
            item.category +
            `
              <div class="text-sm font-normal text-gray-500 mt-1">${item.explain}</div>`;
          sectionDiv.appendChild(titleH3);

          const selectElement = document.createElement("select");
          selectElement.id = item.nameAttr;
          selectElement.className = "bishop-criterion-select";

          const defaultOption = document.createElement("option");
          defaultOption.value = "";
          defaultOption.text = "Selecione...";
          selectElement.appendChild(defaultOption);

          item.options.forEach((option) => {
            const optionElement = document.createElement("option");
            optionElement.value = option.score;
            optionElement.text = `${option.description} (pontos: ${option.score})`;
            selectElement.appendChild(optionElement);
          });

          sectionDiv.appendChild(selectElement);
          conteudoDiv.appendChild(sectionDiv);
        });

        function calcularBishop() {
          const selects = document.querySelectorAll("#conteudo select");
          let allSelected = true;
          let total = 0;

          selects.forEach((select) => {
            if (select.value === "") {
              allSelected = false;
              select.classList.add("incomplete");
            } else {
              select.classList.remove("incomplete");
              total += Number(select.value);
            }
          });

          resultadoDiv.style.display = "block";
          if (!allSelected) {
            resultadoDiv.innerHTML =
              '‚ö†Ô∏è <strong class="font-bold">Por favor, selecione uma op√ß√£o para todos os itens antes de calcular.</strong>';
            resultadoDiv.classList.add("bg-red-100");
            resultadoDiv.classList.remove("bg-yellow-100", "bg-blue-50");
            delete resultadoDiv.dataset.score;
            return;
          }

          let classificacao = "";
          let recomendacao = "";
          let resultadoClasse = "";

          if (total < 6) {
            classificacao = "Colo Desfavor√°vel / Imaturo";
            recomendacao =
              "O colo do √∫tero n√£o est√° maduro. √â prov√°vel que sejam necess√°rios m√©todos de preparo cervical antes de se tentar a indu√ß√£o do trabalho de parto.";
            resultadoClasse = "bg-red-100";
          } else if (total >= 6 && total <= 7) {
            classificacao = "Zona de Incerteza";
            recomendacao =
              "A indu√ß√£o pode ser tentada, mas o sucesso n√£o √© garantido. A monitoriza√ß√£o cuidadosa √© essencial.";
            resultadoClasse = "bg-yellow-100";
          } else {
            // total >= 8
            classificacao = "Colo Favor√°vel / Maduro";
            recomendacao =
              "O colo do √∫tero est√° maduro, com alta probabilidade de sucesso para a indu√ß√£o do parto. O parto espont√¢neo pode estar pr√≥ximo.";
            resultadoClasse = "bg-blue-50";
          }

          resultadoDiv.innerHTML = `
                <p>Escore total: <strong class="font-bold">${total}</strong></p>
                <p>Classifica√ß√£o: <strong class="font-bold">${classificacao}</strong></p>
                <p class="mt-4"><strong>Recomenda√ß√£o:</strong> ${recomendacao}</p>
            `;
          resultadoDiv.className = `resultado ${resultadoClasse}`;
          resultadoDiv.style.display = "block";

          resultadoDiv.dataset.score = total;
          resultadoDiv.dataset.scaleName = "Escala de Bishop";
          resultadoDiv.dataset.classificacao = classificacao;
        }

        function limparCampos() {
          const selects = document.querySelectorAll("#conteudo select");
          selects.forEach((select) => {
            select.selectedIndex = 0;
            select.classList.remove("incomplete");
          });
          resultadoDiv.innerHTML = "";
          resultadoDiv.className = "resultado hidden";
          delete resultadoDiv.dataset.score;
          delete resultadoDiv.dataset.scaleName;
          delete resultadoDiv.dataset.classificacao;
        }

        btnCalcular?.addEventListener("click", calcularBishop);
        btnLimpar?.addEventListener("click", limparCampos);

        btnGerarPDF?.addEventListener("click", () => {
          if (!resultadoDiv.dataset.score) {
            alert(
              'Por favor, clique em "Calcular Escore" primeiro para gerar o PDF.',
            );
            return;
          }
          if (typeof gerarPDFGlobal === "function") {
            gerarPDFGlobal({
              titulo: "Escala de Bishop",
              subtitulo: "Relat√≥rio de Avalia√ß√£o do Colo do √ötero",
              nomeArquivo: "relatorio_escala_bishop.pdf",
              seletorConteudo: ".main-content-wrapper",
            });
          } else {
            console.error("Fun√ß√£o gerarPDFGlobal n√£o definida.");
          }
        });

        btnNandaNicNoc?.addEventListener("click", () => {
          const score = resultadoDiv.dataset.score;
          const scaleName = resultadoDiv.dataset.scaleName;
          const classificacao = resultadoDiv.dataset.classificacao;
          if (!score || !classificacao) {
            alert("Calcule o resultado para pesquisar a conduta.");
            return;
          }
          const searchQuery = encodeURIComponent(
            `${scaleName} pontua√ß√£o ${score} parto ${classificacao} gerar taxonomia NANDA, gerar taxonomia NIC e gerar taxonomia de NOC`,
          );
          window.open(
            `https://www.google.com/search?q=${searchQuery}`,
            "_blank",
          );
        });
      });
    </script>

