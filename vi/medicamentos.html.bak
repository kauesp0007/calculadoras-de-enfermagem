    <script src="global-scripts.js" defer=""></script>

    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const resultadoDiv = document.getElementById("resultado");
        const btnCalcular = document.getElementById("btnCalcular");
        const btnLimpar = document.getElementById("btnLimpar");
        const proporcaoOptionsDiv =
          document.getElementById("proporcao-options");

        // Removida a l√≥gica JavaScript conflitante para que as op√ß√µes est√°ticas (1mg/ml e 2mg/ml) sejam sempre vis√≠veis.

        function calcularMedicamentos() {
          const prescricao = parseFloat(
            document.getElementById("prescricao").value,
          );
          // NOTA: concentracao e volumeTotal s√£o mantidos, mas o c√°lculo de dosagem X (ml) ser√° baseado na propor√ß√£o (mg/ml) selecionada.
          const concentracaoTotal = parseFloat(
            document.getElementById("concentracao").value,
          );
          const volumeTotal = parseFloat(
            document.getElementById("volumeTotal").value,
          );
          const proporcaoRadio = document.querySelector(
            'input[name="proporcao"]:checked',
          );

          resultadoDiv.style.display = "block";

          // Usando apenas Prescricao (P) e a Propor√ß√£o selecionada (C_ratio mg/ml)
          if (isNaN(prescricao) || !proporcaoRadio) {
            resultadoDiv.innerHTML =
              '‚ö†Ô∏è <strong class="font-bold">Por favor, preencha a Prescri√ß√£o M√©dica e selecione a Propor√ß√£o do Frasco.</strong>';
            resultadoDiv.className = "standard-result-box bg-info-orange-light";
            return;
          }

          // C_ratio √© a concentra√ß√£o em mg por 1 ml (valor 1 ou 2 do radio button).
          const cRatio = parseFloat(proporcaoRadio.value); // Ser√° 1 ou 2

          // C√°lculo de dosagem: X (ml) = Prescricao (P) / C_ratio (mg/ml)
          const resultadoMl = prescricao / cRatio;

          const calculoStr = `
  ${cRatio}mg est√° para 1ml
  ${prescricao}mg est√° para X ml

  ${cRatio} * X = ${prescricao} * 1
  X = ${prescricao} / ${cRatio}
  X = ${resultadoMl.toFixed(2)} ml
                `;

          const explicacaoStr = `A prescri√ß√£o √© de ${prescricao} mg. A concentra√ß√£o da solu√ß√£o √© de ${cRatio} mg/ml. Deve-se aspirar ${resultadoMl.toFixed(2)} ml.`;

          resultadoDiv.innerHTML = `
                    <pre>${calculoStr.trim()}</pre>
                    <p class="mt-4 font-sans text-base">${explicacaoStr}</p>
                `;
          resultadoDiv.className = "standard-result-box";
        }

        function limparCampos() {
          document.getElementById("prescricao").value = "";
          document.getElementById("concentracao").value = "";
          document.getElementById("volumeTotal").value = "";

          // Desmarca o r√°dio button
          document
            .querySelectorAll('input[name="proporcao"]')
            .forEach((radio) => {
              radio.checked = false;
            });
          resultadoDiv.innerHTML = "";
          resultadoDiv.className = "hidden";
        }

        btnCalcular?.addEventListener("click", calcularMedicamentos);
        btnLimpar?.addEventListener("click", limparCampos); // Corrigido o erro de digita√ß√£o da fun√ß√£o
      });
    </script>

