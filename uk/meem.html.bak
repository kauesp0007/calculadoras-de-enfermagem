    }

    /* -----------------------------------------------------
       2) FUN√á√ÉO INTERNA: carrega AdSense uma √∫nica vez
       ----------------------------------------------------- */
    function loadAdSenseOnce() {
      if (window.__adsenseLoaded) return;

      window.__adsenseLoaded = true;

      var scriptAd = document.createElement('script');
      scriptAd.async = true;
      scriptAd.src =
