        // --- Fun√ß√µes da Calculadora MEEM ---
        function updateScoreDisplay(criterionId) {
          const selectElement = document.getElementById(criterionId);
          const scoreDisplayElement = document.getElementById(
            `${criterionId}_score_display`,
          );
          if (selectElement && scoreDisplayElement) {
            scoreDisplayElement.innerText =
              selectElement.value === "" ? "0" : selectElement.value;
          }
        }

        function calcularPontuacao() {
          let totalScore = 0;
          let allItemsSelected = true;

          meemData.forEach((item) => {
            const selectElement = document.getElementById(item.nameAttr);
            if (selectElement.value === "") {
              allItemsSelected = false;
            } else {
              totalScore += parseInt(selectElement.value, 10);
            }
          });

          resultadoDiv.style.display = "block";
          if (!allItemsSelected) {
            resultadoDiv.innerHTML =
              '‚ö†Ô∏è <strong class="font-bold">Por favor, selecione uma pontua√ß√£o para cada item</strong> antes de calcular.';
            resultadoDiv.className = "resultado bg-info-orange-light";
            delete resultadoDiv.dataset.score;
            return;
          }

          let classificacao = "";
          let resultadoClasse = "";

          if (totalScore >= 25) {
            classificacao =
              "Nenhum comprometimento cognitivo detectado neste teste";
            resultadoClasse = "bg-meem-normal";
          } else if (totalScore >= 21 && totalScore <= 24) {
            classificacao =
              "Comprometimento Cognitivo Leve apresentado neste teste";
            resultadoClasse = "bg-meem-leve";
          } else if (totalScore >= 10 && totalScore <= 20) {
            classificacao =
              "Comprometimento Cognitivo Moderado apresentado neste teste";
            resultadoClasse = "bg-meem-moderado";
          } else if (totalScore >= 0 && totalScore <= 9) {
            classificacao =
              "Comprometimento Cognitivo Grave apresentado neste teste";
            resultadoClasse = "bg-meem-grave";
          } else {
            classificacao = "Pontua√ß√£o inv√°lida";
            resultadoClasse = "bg-info-orange-light";
          }

          resultadoDiv.innerHTML = `<p>Pontua√ß√£o Total do Mini-Exame do Estado Mental (MEEM): <strong class="font-bold">${totalScore}</strong></p><p>Classifica√ß√£o: <strong class="font-bold">${classificacao}</strong></p>`;
          resultadoDiv.className = `resultado ${resultadoClasse}`;

          resultadoDiv.dataset.score = totalScore;
          resultadoDiv.dataset.classificacao = classificacao;
          resultadoDiv.dataset.scaleName = "Mini-Exame do Estado Mental (MEEM)";
        }

        function limparCampos() {
          const selects = document.querySelectorAll("#conteudo select");
          const pdfTexts = document.querySelectorAll(
            ".selected-option-for-pdf",
          );
          selects.forEach((select) => {
            select.selectedIndex = 0;
            // Dispara o evento onchange para resetar o score display e o texto do PDF
            select.dispatchEvent(new Event("change"));
          });
          pdfTexts.forEach((textEl) => {
            textEl.innerText = ""; // Limpa o texto do PDF
          });
          resultadoDiv.innerHTML = "";
          resultadoDiv.className = "resultado hidden";
          delete resultadoDiv.dataset.score;
          delete resultadoDiv.dataset.classificacao;
          delete resultadoDiv.dataset.scaleName;
        }

        // Preenche o formul√°rio dinamicamente
        meemData.forEach((item, index) => {
          const sectionDiv = document.createElement("div");
          sectionDiv.className = "meem-criterion-card";

          const headerDiv = document.createElement("div");
          headerDiv.className = "meem-criterion-header";

          const titleH3 = document.createElement("h3");
          titleH3.className = "meem-criterion-title";
          titleH3.innerText = item.domain;
          headerDiv.appendChild(titleH3);

          const scoreSpan = document.createElement("span");
          scoreSpan.className = "meem-score-display";
          scoreSpan.id = `${item.nameAttr}_score_display`;
          scoreSpan.innerText = "0";
          headerDiv.appendChild(scoreSpan);

          sectionDiv.appendChild(headerDiv);

          const instructionsP = document.createElement("p");
          instructionsP.className = "text-gray-700 mb-4 text-left";
          instructionsP.innerText = item.instructions;
          sectionDiv.appendChild(instructionsP);

          if (item.type === "image" && item.imageSrc) {
            const imgDiv = document.createElement("div");
            imgDiv.className = "image-cell mb-4 text-center";
            const img = document.createElement("img");
            img.src = item.imageSrc; // O caminho j√° est√° relativo √† raiz
            img.alt = `Diagrama para ${item.domain}`;
            img.classList.add(
              "w-auto",
              "h-auto",
              "max-w-[200px]",
              "mx-auto",
              "rounded-md",
              "shadow-sm",
            );
            imgDiv.appendChild(img);
            sectionDiv.appendChild(imgDiv);
          }

          const optionsDiv = document.createElement("div");
          optionsDiv.className = "meem-criterion-options";

          const selectElement = document.createElement("select");
          selectElement.id = item.nameAttr;
          selectElement.className = "meem-select";

          // PDF FIX: Cria o elemento de texto para o PDF
          const pdfSelectionText = document.createElement("p");
          pdfSelectionText.id = `${item.nameAttr}_pdf_selection`;
          pdfSelectionText.className = "selected-option-for-pdf";

          // PDF FIX: Atualiza o score E o texto do PDF no evento onchange
          selectElement.onchange = () => {
            updateScoreDisplay(item.nameAttr);
            const selectedIndex = selectElement.selectedIndex;
            if (selectedIndex > 0) {
              // Maior que 0 para ignorar o "Selecione..."
              const selectedText = selectElement.options[selectedIndex].text;
              pdfSelectionText.innerText = `Sele√ß√£o: ${selectedText}`;
            } else {
              pdfSelectionText.innerText = ""; // Limpa se voltar para "Selecione..."
            }
          };

          const defaultOption = document.createElement("option");
          defaultOption.value = "";
          defaultOption.text = "Selecione...";
          defaultOption.disabled = true;
          defaultOption.selected = true;
          selectElement.appendChild(defaultOption);

          item.scoreOptions.forEach((option) => {
            const optionElement = document.createElement("option");
            optionElement.value = option.score;
            optionElement.text = `${option.description} (${option.score})`;
            selectElement.appendChild(optionElement);
          });

          optionsDiv.appendChild(selectElement);
          optionsDiv.appendChild(pdfSelectionText); // PDF FIX: Adiciona o elemento de texto
          sectionDiv.appendChild(optionsDiv);
          conteudoDiv.appendChild(sectionDiv);
        });

        // Adiciona os event listeners aos bot√µes
        btnCalcular?.addEventListener("click", calcularPontuacao);
        btnLimpar?.addEventListener("click", limparCampos);

        btnGerarPDF?.addEventListener("click", () => {
          if (!resultadoDiv.dataset.score) {
            alert('Por favor, clique em "Calcular" primeiro para gerar o PDF.');
            return;
          }
          if (typeof gerarPDFGlobal === "function") {
            gerarPDFGlobal({
              titulo: "Mini-Exame do Estado Mental (MEEM)",
              subtitulo: "Relat√≥rio de Rastreio Cognitivo",
              nomeArquivo: "relatorio_escala_meem.pdf",
              seletorConteudo: ".main-content-wrapper",
            });
          } else {
            console.error(
              "Fun√ß√£o gerarPDFGlobal n√£o definida. Verifique se global-scripts.js est√° carregando.",
            );
            alert(
              "Erro ao gerar PDF. Verifique o console para mais detalhes.",
            );
          }
        });

        btnNandaNicNoc?.addEventListener("click", () => {
          const score = resultadoDiv.dataset.score;
          const scaleName = resultadoDiv.dataset.scaleName;
          const classificacao = resultadoDiv.dataset.classificacao;
          if (!score || !classificacao) {
            alert("Calcule o resultado para pesquisar a conduta.");
            return;
          }
          const searchQuery = encodeURIComponent(
            `${scaleName} pontua√ß√£o ${score} classifica√ß√£o ${classificacao} gerar taxonomia NANDA, gerar taxonomia NIC e gerar taxonomia de NOC`,
          );
          window.open(
            `https://www.google.com/search?q=${searchQuery}`,
            "_blank",
          );
        });
        // Bot√£o "Adicionar aos favoritos"
        const addFavoriteBtn = document.getElementById("addFavoriteBtn");
        if (addFavoriteBtn) {
          addFavoriteBtn.addEventListener("click", () => {
            const url = window.location.href;
            const title = "Escala de Meem";

            try {
              // IE/Edge Legacy
              if (window.external && "AddFavorite" in window.external) {
                window.external.AddFavorite(url, title);
                return;
              }
              // Firefox antigo
              if (window.sidebar && window.sidebar.addPanel) {
                window.sidebar.addPanel(title, url, "");
                return;
              }
              // Chrome/Firefox modernos: instru√ß√£o de atalho
              alert(
                "Pressione Ctrl + D (ou Cmd + D no Mac) para adicionar esta p√°gina aos favoritos.",
              );
            } catch (e) {
              alert(
                "Pressione Ctrl + D (ou Cmd + D no Mac) para adicionar esta p√°gina aos favoritos.",
              );
            }
          });
        }
      });
    </script>

