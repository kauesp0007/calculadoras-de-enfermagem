    <!-- Script Espec√≠fico da P√°gina FAST -->
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const resultadoDiv = document.getElementById("resultado");
        const btnCalcular = document.getElementById("btnCalcular");
        const btnLimpar = document.getElementById("btnLimpar");
        const btnGerarPDF = document.getElementById("btnGerarPDF");
        const btnNandaNicNoc = document.getElementById("btnNandaNicNoc");

        function calcular() {
          const faceChecked = document.querySelector(
            'input[name="face"]:checked',
          );
          const armChecked = document.querySelector(
            'input[name="arm"]:checked',
          );
          const speechChecked = document.querySelector(
            'input[name="speech"]:checked',
          );

          let resultadoTexto = "";
          let classificacao = "";
          let resultadoClasse = "";

          if (faceChecked || armChecked || speechChecked) {
            resultadoTexto =
              "Sinais de AVE identificados! Acionar equipe m√©dica imediatamente.";
            classificacao = "Sinais FAST de AVE Presentes";
            resultadoClasse = "bg-fast-positivo";
          } else {
            resultadoTexto =
              "Nenhum sinal de AVE foi selecionado. O paciente n√£o apresenta sinais FAST de AVE.";
            classificacao = "Sem Sinais FAST de AVE";
            resultadoClasse = "bg-fast-negativo";
          }

          resultadoDiv.innerHTML = `<p><strong class="font-bold">${resultadoTexto}</strong></p>`;
          resultadoDiv.className = `resultado ${resultadoClasse}`;
          resultadoDiv.style.display = "block";

          resultadoDiv.dataset.score = classificacao;
          resultadoDiv.dataset.scaleName = "Escala FAST";
        }

        function limpar() {
          document
            .querySelectorAll('input[type="checkbox"]')
            .forEach((checkbox) => (checkbox.checked = false));
          resultadoDiv.innerHTML = "";
          resultadoDiv.className = "resultado hidden";
          delete resultadoDiv.dataset.score;
          delete resultadoDiv.dataset.scaleName;
        }

        btnCalcular?.addEventListener("click", calcular);
        btnLimpar?.addEventListener("click", limpar);

        btnGerarPDF?.addEventListener("click", () => {
          if (!resultadoDiv.dataset.score) {
            alert('Por favor, clique em "Calcular" primeiro para gerar o PDF.');
            return;
          }
          if (typeof gerarPDFGlobal === "function") {
            gerarPDFGlobal({
              titulo: "Escala FAST",
              subtitulo: "Relat√≥rio de Triagem R√°pida de AVE",
              nomeArquivo: "relatorio_escala_fast.pdf",
              seletorConteudo: ".main-content-wrapper",
            });
          } else {
            console.error("Fun√ß√£o gerarPDFGlobal n√£o definida.");
          }
        });

        btnNandaNicNoc?.addEventListener("click", () => {
          const score = resultadoDiv.dataset.score;
          const scaleName = resultadoDiv.dataset.scaleName;
          if (!score) {
            alert("Calcule o resultado para pesquisar a conduta.");
            return;
          }
          const searchQuery = encodeURIComponent(
            `${score} ${scaleName} gerar taxonomia de NANDA gerar taxonomia de NIC gerar Taxonomia de NOC`,
          );
          window.open(
            `https://www.google.com/search?q=${searchQuery}`,
            "_blank",
          );
        });
      });
    </script>

